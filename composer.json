{
    "name": "julianmc90/webbake",
    "description": "webbake plugin for cakephp",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
 
    "autoload": {
        "psr-4": {
            "WebBake\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebBake\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
